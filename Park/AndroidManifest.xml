        <activity
            android:name="com.example.park.MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name="com.example.park.ParkActivity"
            android:label="@string/title_activity_park" >
        </activity>
        <activity
            android:name="com.example.park.InfoTabActivity"
            android:label="@string/title_activity_info_tab" >
        </activity>
        <activity
