        <activity
            android:name="com.example.park.Fragment2"
            android:label="@string/title_activity_fragment2" >
        </activity>
        <activity
            android:name="com.example.park.ShowFragResultActivity"
            android:label="@string/title_activity_show_frag_result" >
        </activity>
        <activity
            android:name="com.example.park.SearchPageActivity"
            android:label="@string/title_activity_search_page"
