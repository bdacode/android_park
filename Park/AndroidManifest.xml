            android:label="@string/title_activity_info_one" >
        </activity>
        <activity
            android:name="com.example.park.InfoTwoActivity"
            android:label="@string/title_activity_info_two" >
        </activity>
        <activity
            android:name="com.example.park.InfoFragmentActivity"
            android:label="@string/title_activity_info_fragment" >
        </activity>
        <activity
            android:name="com.example.park.Fragment1"
            android:label="@string/title_activity_fragment1"
            android:windowSoftInputMode="stateVisible" >
